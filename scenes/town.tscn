var isolado:String = \"\"
var selecionado:String = \"\"
var dificuldade:int
var listaPessoas:Array

# Called when the node enters the scene tree for the first time.
func _ready():
	dificuldade = get_node(\"/root/Configuracoes\").dificuldade
	_inicializaPessoas()
	_inicializarPacienteZero()


func _inicializaPessoas():
	listaPessoas = get_children()
	if(dificuldade >= 1):
		listaPessoas+=get_parent().get_node(\"PopulationDificil\").get_children()
	if (dificuldade == 2):
		listaPessoas+=get_parent().get_node(\"PopulationMuitoDificil\").get_children()
	for n in listaPessoas :
		randomize()
		n.imunidade = rand_range(3,5)
		n.get_child(0).show()

func _inicializarPacienteZero():
	randomize()
	var sorteado = rand_range(0, listaPessoas.size())
	listaPessoas[sorteado].pacienteZero = true





# Isolar pessoa

func _on_Button_button_down():
		for p in listaPessoas:
			if(p.name == selecionado && !p.morta):
				isolado = selecionado
				get_parent().get_node(\"display-canvas-bg/pessoaIsolada\").set_text(selecionado)


func _on_dificil_ready():
	if(get_node(\"/root/Configuracoes\").dificuldade < 1):
		get_parent().get_node(\"Fichas/Arquivo/dificil\").hide()
	pass # Replace with function body.


func _on_muitoDificil_ready():
	if(get_node(\"/root/Configuracoes\").dificuldade < 2):
		get_parent().get_node(\"Fichas/Arquivo/muitoDificil\").hide()
	pass # Replace with function body.
