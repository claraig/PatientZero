script/source = "extends MarginContainer
#
var scene_path

func _ready():
	$cont1/esq/menu_buttons/bNewGame.grab_focus()
#	Navegar usando o teclado


func _on_bNewGame_pressed():
	get_node('cont1/esq/menu_buttons/audioKey').play()
	scene_path = \"res://scenes/town.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_bContinue_pressed():
	get_node('cont1/esq/menu_buttons/audioKey').play()
	scene_path = \"res://scenes/Menu/Instructions.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_bOptions_pressed():
	get_node('cont1/esq/menu_buttons/audioKey').play()
	scene_path = \"res://scenes/Menu/Options.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_FadeIn_fade_finished():
	get_tree().change_scene(scene_path)
"

