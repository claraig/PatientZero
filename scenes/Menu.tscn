script/source = "extends Node2D

var scene_path

func _on_FadeIn_fade_finished():
	get_tree().change_scene(scene_path)

func _on_novojogo_pressed():
	get_node('Sons/click').play()
	scene_path = \"res://scenes/town.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_instrucoes_pressed():
	get_node('Sons/click').play()
	scene_path = \"res://scenes/Menu/Instructions.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()


func _on_opcoes_pressed():
	get_node('Sons/click').play()
	scene_path = \"res://scenes/Menu/Options.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()


func _on_creditos_pressed():
	get_node('Sons/click').play()
	scene_path = \"res://scenes/Menu/Creditos.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()


func _on_sair_pressed():
	get_node('Sons/click').play()
	get_tree().quit()

#____________________ OBJETOS ____________________

func _on_arq_button_down():
	$Sons/arq.play()
	$arq/gav/AnimationPlayer.play('mov')

func _on_cafeteira_toggled(button_pressed: bool):
	if button_pressed:
		$cafeteira/cafeteira2.show()
		$Sons/cafe.play()
	else:
		$cafeteira/cafeteira2.hide()
		$Sons/cafe.stop()

func _on_aluminaria_pressed():
	$Sons/bzz.play()
	$luminariaA/luminariaB/AnimationPlayer.play('mov')

func _on_telefone_pressed():
	$Sons/fone.play(1)
	$telefone/Sprite/AnimationPlayer.play('mov')

func _on_caneta_pressed():
	get_node('porta lapis/canetas/AnimationPlayer').play('mov')
	$Sons/canetas.play()

func _on_caixa_pressed():
	$Sons/caixa.play()
	$caixa/caixa2/AnimationPlayer.play('mov')
"

