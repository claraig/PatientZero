script/source = "extends Node2D

var scene_path

func _on_FadeIn_fade_finished():
	get_tree().change_scene(scene_path)

func _on_novojogo_pressed() -> void:
	get_node('click').play()
	scene_path = \"res://scenes/town.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_instrucoes_pressed() -> void:
	get_node('click').play()
	scene_path = \"res://scenes/Menu/Instructions.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_opcoes_pressed() -> void:
	get_node('click').play()

#	get_node('res://scenes/Menu/Options.tscn').get_node('CheckButton1')

	scene_path = \"res://scenes/Menu/Options.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

#func _on_creditos_pressed() -> void:
#	get_node('click').play()
#	pass # Replace with function body.


func _on_sair_pressed() -> void:
	get_node('click').play()
	get_tree().quit()
"

