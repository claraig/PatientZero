var scene_path

func _on_FadeIn_fade_finished():
	get_tree().change_scene(scene_path)

func _on_novojogo_pressed() -> void:
	get_node('click').play()
	scene_path = \"res://scenes/town.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()

func _on_instrucoes_pressed() -> void:
	get_node('click').play()
	scene_path = \"res://scenes/Menu/Instructions.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()


func _on_opcoes_pressed() -> void:
	get_node('click').play()
	scene_path = \"res://scenes/Menu/Options.tscn\"
	$FadeIn.show()
	$FadeIn.fade_in()


